{
  "presets": [
    "babel-preset-react",
    [
      "babel-preset-env",
      {
      "targets": {
        "ie9": true,
        "uglify": true,
        "modules": false
        }
      }
    ]
  ],
  "plugins": [
    "transform-object-rest-spread",
    "babel-plugin-transform-class-properties",
    "babel-plugin-syntax-dynamic-import",
    "babel-plugin-transform-export-extensions"
  ]
}