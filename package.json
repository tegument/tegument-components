{
  "name": "tegument-components",
  "version": "1.0.3",
  "description": "Tegument Build Components",
  "main": "dist/index.js",
  "watch": {
    "transpile": "{src,test}/**/*.js"
  },
  "scripts": {
    "dev": "webpack-dev-server --mode development",
    "start": "yarn run dev",
    "transpile": "babel src -d dist --copy-files",
    "watch:transpile": "npm-watch",
    "test": "echo \"No tests configured\"",
    "clean": "rimraf ./dist/*",
    "prepublishOnly": "yarn run clean && yarn run transpile",
    "build": "webpack --mode production",
    "deploy": "yarn run styleguide:build && gh-pages -d styleguide",
    "publish-demo": "npm run build && npm run deploy",
    "prepush": "yarn run lint",
    "lint": "standard ./src",
    "lint:fix": "standard --fix",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tegument/tegument-components.git"
  },
  "author": "Spencer Thornock",
  "license": "MIT",
  "peerDependancies": {
    "react": ">=0.14",
    "react-dom": "^16.7.0"
  },
  "devDependencies": {
    "aphrodite": "^2.1.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "7.1.4",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "cssnano": "^4.1.8",
    "eslint": "^3.19.0",
    "eslint-config-prettier": "^2.3.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.0.1",
    "eslint-plugin-standard": "^3.0.1",
    "fabric": "^2.6.0",
    "file-loader": "^3.0.1",
    "gh-pages": "^2.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "lodash": "^4.17.11",
    "prop-types": "^15.6.2",
    "react": ">=0.14",
    "react-dates": "^18.3.1",
    "react-dom": "^16.7.0",
    "react-styleguidist": "^8.0.6",
    "react-with-direction": "^1.1.0",
    "react-with-styles": "^3.0.0",
    "react-with-styles-interface-aphrodite": "^5.0.1",
    "rimraf": "^2.6.3",
    "semantic-release": "^15.13.3",
    "style-loader": "^0.23.1",
    "webpack": "^4.28.4",
    "webpack-blocks": "^1.0.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  },
  "standard": {
    "parser": "babel-eslint",
    "rules": {
      "node/no-deprecated-api": [
        "error",
        {
          "ignoreModuleItems": [
            "constants"
          ],
          "ignoreGlobalItems": [
            "constants"
          ]
        }
      ]
    }
  },
  "dependencies": {
    "codecov": "^3.1.0",
    "npm-watch": "^0.5.0",
    "recompose": "^0.30.0"
  }
}
